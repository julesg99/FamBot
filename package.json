{
  "name": "fambot",
  "version": "0.0.5",
  "description": "Discord bot for Fam activites",
  "main": "./dist/index.js",
  "scripts": {
    "start": "tsc && node ./dist/deploy-commands.js && node ./dist/index.js",
    "boot": "node ./dist/index.js",
    "deploy": "node ./dist/deploy-commands.js",
    "dcu": "docker compose up -d",
    "dcd": "docker compose down",
    "dsp": "docker system prune -a --volumes -f && docker volume rm $(docker volume ls -qf dangling=true | xargs)",
    "clean": "rm -rf node_modules"
  },
  "keywords": [
    "discord",
    "bot",
    "film",
    "fam"
  ],
  "author": "Jules Garver",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "discord.js": "^14.15.2",
    "discord-api-types": "^0.37.119",
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "tslib": "^2.4.1",
    "ts-node": "^10.9.1",
    "prettier": "^3.2.5",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-config-prettier": "^8.8.0",
    "@typescript-eslint/parser": "^8.24.1",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@types/node": "^22.0.0",
    "@types/debug": "^4.1.12",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"prettier-plugin-svelte": "^3.2.1"
  }
}
