{
  "name": "fambot",
  "version": "0.0.1",
  "description": "discord bot for fam film night",
  "main": "index.js",
  "scripts": {
    "start": "node deploy-commands.js && node index.js",
    "boot": "node index.js",
    "deploy": "node deploy-commands.js",
    "dcu": "docker compose up -d",
    "dcd": "docker compose down",
    "dsp": "docker system prune -a --volumes -f && docker volume rm $(docker volume ls -qf dangling=true | xargs)",
    "clean": "rm -rf node_modules"
  },
  "keywords": [
    "discord",
    "bot",
    "film",
    "fam"
  ],
  "author": "Juulz",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  }
}
